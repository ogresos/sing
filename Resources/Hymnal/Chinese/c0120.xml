<hymn xmlns="http://www.hymnal.net/hymn">
  <title>哦主你是有的主</title>
  <index>true</index>
  <category>讚美主－祂的榮耀</category>
  <numbers><cebuano num="133"/><chinese num="120"/><english num="133"/><german num="133"/><korean num="120"/><portuguese num="70"/><russian num="403"/><spanish num="69"/><tagalog num="133"/></numbers>
