<hymn xmlns="http://www.hymnal.net/hymn">
  <title>主你乃是王中王</title>
  <index>true</index>
  <category>讚美主－祂的國度</category>
  <numbers><cebuano num="148"/><chinese num="124"/><english num="148"/><german num="148"/><korean num="124"/><portuguese num="74"/><russian num="345"/><spanish num="74"/><tagalog num="148"/></numbers>
