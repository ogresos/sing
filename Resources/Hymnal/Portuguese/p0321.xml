<hymn xmlns="http://www.hymnal.net/hymn">
  <title>Se ressurreição anelo</title>
  <index>true</index>
  <category>O Caminho Da Vida</category>
  <numbers><cebuano num="631"/><chinese num="464"/><english num="631"/><german num="631"/><korean num="464"/><portuguese num="321"/><russian num="244"/><spanish num="297"/><tagalog num="631"/></numbers>
