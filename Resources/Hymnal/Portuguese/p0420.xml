<hymn xmlns="http://www.hymnal.net/hymn">
  <title>Cristo é Vitorioso</title>
  <index>true</index>
  <category>Pela Vitória Do Senhor</category>
  <numbers><cebuano num="890"/><chinese num="644"/><dutch num="71"/><english num="890"/><german num="890"/><korean num="644"/><portuguese num="420"/><russian num="145"/><spanish num="401"/><tagalog num="890"/></numbers>
