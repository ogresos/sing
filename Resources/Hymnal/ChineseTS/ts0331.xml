<hymn xmlns="http://www.hymnal.net/hymn">
  <title>我是屬主</title>
  <index>true</index>
  <category>愛慕耶穌</category>
  <numbers><cebuano num="433"/><chinese num="ts:331"/><english num="433"/><french num="74"/><portuguese num="200"/><russian num="215"/><spanish num="184"/><tagalog num="433"/></numbers>
